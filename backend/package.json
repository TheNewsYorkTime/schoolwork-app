{
	"name": "schoolwork-app",
	"version": "1.0.0",
	"description": "A proxy service to evade the site blocking on school chromebooks",
	"main": "app.js",
	"private": true,
	"scripts": {
		"dev": "nodemon --exec babel-node ./src/app.js",
		"clean": "rm -rf dist",
		"build": "npm run clean && babel ./src --out-dir dist --copy-files",
		"prod": "node ./dist/app.js"
	},
	"engines": {
		"node": "^18.0.0"
	},
	"author": "Ashes Lawless",
	"license": "ISC",
	"dependencies": {
		"cookie-parser": "~1.4.4",
		"core-js": "^3.33.3",
		"debug": "~2.6.9",
		"express": "^4.18.2",
		"http-errors": "~1.6.3",
		"morgan": "~1.9.1",
		"regenerator-runtime": "^0.14.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.23.4",
		"@babel/core": "^7.23.3",
		"@babel/node": "^7.22.19",
		"@babel/preset-env": "^7.23.3",
		"nodemon": "^3.0.1"
	}
}
